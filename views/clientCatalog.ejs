<%- include('header'); -%>
    <div class="content">
        <h1>Client List</h1>
        <table id="catalog">
            <thead>
                <tr>
                    <th scope="col">num_client</th>
                    <th scope="col">contact</th>
                    <th scope="col">phone</th>
                </tr>
            </thead>
            <tbody>
                <% clientslist.forEach(client => { %>
                    <tr>
                        <th scope="row">
                            <%= client.num_client %>
                        </th>
                        <td>
                            <%= client.contact %>
                            </a>

                        </td>
                        <td>
                            <%= client.phone %>
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>
    <br>
    <%- include('footer'); -%>